{
  "name": "xml-workflow-design",
  "productName": "XML Workflow Design",
  "version": "0.1.0",
  "scripts": {
    "ng": "ng",
    "start": "concurrently \"ng serve\" \"npm run electron\"",
    "build": "ng build",
    "build:prod": "ng build --prod",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "electron": "ng build && electron ./src/electron.dev",
    "electron-package:mac": "ng build && cross-var electron-packager ./dist/xml-workflow-design $npm_package_name-$npm_package_version --overwrite --platform=darwin --out=release-build --arch=x64 --icon=dist/xml-workflow-design/assets/brand/suite.icns",
    "electron-package:win": "ng build && cross-var electron-packager ./dist/xml-workflow-design $npm_package_name-$npm_package_version --overwrite --platform=win32 --out=release-build --arch=all --icon=dist/xml-workflow-design/assets/brand/suite.ico",
    "electron-package:linux": "ng build && cross-var electron-packager ./dist/xml-workflow-design $npm_package_name-$npm_package_version --overwrite --platform=linux --out=release-build --arch=all --icon=dist/xml-workflow-design/assets/brand/suite.icns",
    "electron-package:all": "ng build --prod && cross-var electron-packager ./dist/xml-workflow-design $npm_package_name-$npm_package_version --overwrite --all --out=release-build --arch=all --icon=dist/xml-workflow-design/assets/brand/suite.icns"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.1.10",
    "@angular/cdk": "^6.4.7",
    "@angular/common": "^6.1.10",
    "@angular/compiler": "^6.1.10",
    "@angular/core": "^6.1.10",
    "@angular/forms": "^6.1.10",
    "@angular/http": "^6.1.10",
    "@angular/material": "^6.4.7",
    "@angular/platform-browser": "^6.1.10",
    "@angular/platform-browser-dynamic": "^6.1.10",
    "@angular/router": "^6.1.10",
    "@material/theme": "^0.35.0",
    "angular-datatables": "^6.0.1",
    "angular-font-awesome": "^3.1.2",
    "angular-in-memory-web-api": "^0.6.1",
    "angular-tree-component": "^7.2.1",
    "bootstrap": "^4.3.1",
    "core-js": "^2.6.9",
    "datatables.net": "^1.10.19",
    "datatables.net-dt": "^1.10.19",
    "font-awesome": "^4.7.0",
    "jquery": "^3.4.1",
    "ngx-file-drop": "^4.0.6",
    "rxjs": "^6.5.2",
    "zone.js": "^0.8.29"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.800.1",
    "@angular/cli": "~6.0.8",
    "@angular/compiler-cli": "^6.1.10",
    "@angular/language-service": "^6.1.10",
    "@types/datatables.net": "^1.10.17",
    "@types/jasmine": "^2.8.16",
    "@types/jasminewd2": "^2.0.6",
    "@types/jquery": "^3.3.29",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.2.1",
    "concurrently": "^3.6.1",
    "cross-var": "^1.1.0",
    "electron": "^2.0.18",
    "electron-packager": "^16.0.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^3.1.4",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^2.0.5",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "^5.4.2",
    "ts-node": "~5.0.1",
    "tslint": "~5.9.1",
    "typescript": "~2.7.2"
  }
}
